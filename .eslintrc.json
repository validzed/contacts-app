{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["plugin:react/recommended", "airbnb"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "eol-last": "off",
    "linebreak-style": "off",
    "no-alert": "off",
    "arrow-body-style": ["error", "as-needed"],
    "react/jsx-props-no-spreading": "off"
  }
}
